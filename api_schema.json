    "components": {
        "schemas": {
            "Collectible": {
                "type": "object",
                "description": "Serializer for the Collectible model.\nHandles converting Collectible objects to JSON and validating/converting\nincoming JSON data back into Collectible objects.",
                "properties": {
                    "id": {
                        "type": "integer",
                        "readOnly": true
                    },
                    "name": {
                        "type": "string",
                        "description": "The common name of the collectible item.",
                        "maxLength": 255
                    },
                    "sku": {
                        "type": "string",
                        "description": "Stock Keeping Unit (Unique Identifier).",
                        "maxLength": 50
                    },
                    "location": {
                        "type": "string",
                        "nullable": true,
                        "description": "Physical location (e.g., 'Binder 3', 'Shelf A')",
                        "maxLength": 100
                    },
                    "image": {
                        "type": "string",
                        "format": "uri",
                        "nullable": true,
                        "description": "Image of the collectible item."
                    },
                    "quantity": {
                        "type": "integer",
                        "maximum": 2147483647,
                        "minimum": -2147483648,
                        "description": "Current number of units in stock."
                    },
                    "last_updated": {
                        "type": "string",
                        "format": "date-time",
                        "readOnly": true
                    },
                    "intake_price": {
                        "type": "string",
                        "format": "decimal",
                        "pattern": "^-?\\d{0,8}(?:\\.\\d{0,2})?$",
                        "description": "The price paid for the item (cost basis)."
                    },
                    "current_price": {
                        "type": "string",
                        "format": "decimal",
                        "pattern": "^-?\\d{0,8}(?:\\.\\d{0,2})?$",
                        "description": "The current market value or selling price."
                    },
                    "projected_price": {
                        "type": "string",
                        "format": "decimal",
                        "pattern": "^-?\\d{0,8}(?:\\.\\d{0,2})?$",
                        "description": "The projected price based on market trends."
                    },
                    "description": {
                        "type": "string",
                        "nullable": true
                    },
                    "user": {
                        "type": "integer",
                        "nullable": true,
                        "description": "The user/vendor who owns this inventory item."
                    },
                    "vendor": {
                        "type": "integer",
                        "nullable": true
                    }
                },
                "required": [
                    "id",
                    "last_updated",
                    "name",
                    "sku"
                ]
            },
            "PatchedCollectible": {
                "type": "object",
                "description": "Serializer for the Collectible model.\nHandles converting Collectible objects to JSON and validating/converting\nincoming JSON data back into Collectible objects.",
                "properties": {
                    "id": {
                        "type": "integer",
                        "readOnly": true
                    },
                    "name": {
                        "type": "string",
                        "description": "The common name of the collectible item.",
                        "maxLength": 255
                    },
                    "sku": {
                        "type": "string",
                        "description": "Stock Keeping Unit (Unique Identifier).",
                        "maxLength": 50
                    },
                    "location": {
                        "type": "string",
                        "nullable": true,
                        "description": "Physical location (e.g., 'Binder 3', 'Shelf A')",
                        "maxLength": 100
                    },
                    "image": {
                        "type": "string",
                        "format": "uri",
                        "nullable": true,
                        "description": "Image of the collectible item."
                    },
                    "quantity": {
                        "type": "integer",
                        "maximum": 2147483647,
                        "minimum": -2147483648,
                        "description": "Current number of units in stock."
                    },
                    "last_updated": {
                        "type": "string",
                        "format": "date-time",
                        "readOnly": true
                    },
                    "intake_price": {
                        "type": "string",
                        "format": "decimal",
                        "pattern": "^-?\\d{0,8}(?:\\.\\d{0,2})?$",
                        "description": "The price paid for the item (cost basis)."
                    },
                    "current_price": {
                        "type": "string",
                        "format": "decimal",
                        "pattern": "^-?\\d{0,8}(?:\\.\\d{0,2})?$",
                        "description": "The current market value or selling price."
                    },
                    "projected_price": {
                        "type": "string",
                        "format": "decimal",
                        "pattern": "^-?\\d{0,8}(?:\\.\\d{0,2})?$",
                        "description": "The projected price based on market trends."
                    },
                    "description": {
                        "type": "string",
                        "nullable": true
                    },
                    "user": {
                        "type": "integer",
                        "nullable": true,
                        "description": "The user/vendor who owns this inventory item."
                    },
                    "vendor": {
                        "type": "integer",
                        "nullable": true
                    }
                }
            },
            "TokenObtainPair": {
                "type": "object",
                "properties": {
                    "username": {
                        "type": "string",
                        "writeOnly": true
                    },
                    "password": {
                        "type": "string",
                        "writeOnly": true
                    },
                    "access": {
                        "type": "string",
                        "readOnly": true
                    },
                    "refresh": {
                        "type": "string",
                        "readOnly": true
                    }
                },
                "required": [
                    "access",
                    "password",
                    "refresh",
                    "username"
                ]
            },
            "TokenRefresh": {
                "type": "object",
                "properties": {
                    "access": {
                        "type": "string",
                        "readOnly": true
                    },
                    "refresh": {
                        "type": "string"
                    }
                },
                "required": [
                    "access",
                    "refresh"
                ]
            },
            "TokenVerify": {
                "type": "object",
                "properties": {
                    "token": {
                        "type": "string",
                        "writeOnly": true
                    }
                },
                "required": [
                    "token"
                ]
            }
        },
        "securitySchemes": {
            "jwtAuth": {
                "type": "http",
                "scheme": "bearer",
                "bearerFormat": "JWT"
            }
        }
    }
