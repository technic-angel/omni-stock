{
  "components": {
    "schemas": {
      "CardDetails": {
        "description": "Serializer for the CardDetails nested object.",
        "properties": {
          "condition": {
            "description": "Human-readable condition (e.g., Mint, Near Mint).",
            "maxLength": 50,
            "nullable": true,
            "type": "string"
          },
          "external_ids": {
            "description": "JSON string of external IDs (portable across DBs)",
            "nullable": true,
            "type": "string"
          },
          "language": {
            "maxLength": 50,
            "nullable": true,
            "type": "string"
          },
          "last_estimated_at": {
            "format": "date-time",
            "nullable": true,
            "type": "string"
          },
          "market_region": {
            "maxLength": 100,
            "nullable": true,
            "type": "string"
          },
          "notes": {
            "description": "Internal notes about the card",
            "nullable": true,
            "type": "string"
          },
          "print_run": {
            "description": "Optional print run or edition info.",
            "maxLength": 100,
            "nullable": true,
            "type": "string"
          },
          "psa_grade": {
            "description": "Numeric grade, e.g. 8.5 or 10.0",
            "format": "decimal",
            "nullable": true,
            "pattern": "^-?\\d{0,2}(?:\\.\\d{0,1})?$",
            "type": "string"
          },
          "release_date": {
            "format": "date",
            "nullable": true,
            "type": "string"
          }
        },
        "type": "object"
      },
      "Collectible": {
        "description": "Serializer for the Collectible model with nested card details support.",
        "properties": {
          "card_details": {
            "$ref": "#/components/schemas/CardDetails"
          },
          "category": {
            "description": "High-level category (e.g., TCG, Video Games, Collectibles).",
            "maxLength": 100,
            "nullable": true,
            "type": "string"
          },
          "condition": {
            "description": "Human-readable condition (e.g., Mint, Near Mint).",
            "maxLength": 100,
            "nullable": true,
            "type": "string"
          },
          "created_at": {
            "format": "date-time",
            "readOnly": true,
            "type": "string"
          },
          "description": {
            "nullable": true,
            "type": "string"
          },
          "id": {
            "readOnly": true,
            "type": "integer"
          },
          "image_url": {
            "description": "Public image URL stored in Supabase.",
            "format": "uri",
            "maxLength": 200,
            "nullable": true,
            "type": "string"
          },
          "intake_price": {
            "description": "The price paid for the item (cost basis).",
            "format": "decimal",
            "pattern": "^-?\\d{0,8}(?:\\.\\d{0,2})?$",
            "type": "string"
          },
          "name": {
            "description": "The common name of the collectible item.",
            "maxLength": 255,
            "type": "string"
          },
          "price": {
            "description": "The current market value or selling price.",
            "format": "decimal",
            "pattern": "^-?\\d{0,8}(?:\\.\\d{0,2})?$",
            "type": "string"
          },
          "projected_price": {
            "description": "The projected price based on market trends.",
            "format": "decimal",
            "pattern": "^-?\\d{0,8}(?:\\.\\d{0,2})?$",
            "type": "string"
          },
          "quantity": {
            "description": "Current number of units in stock.",
            "format": "int64",
            "maximum": 9223372036854775807,
            "minimum": -9223372036854775808,
            "type": "integer"
          },
          "sku": {
            "description": "Stock Keeping Unit (Unique Identifier).",
            "maxLength": 50,
            "type": "string"
          },
          "updated_at": {
            "format": "date-time",
            "readOnly": true,
            "type": "string"
          },
          "user": {
            "description": "The user/vendor who owns this inventory item.",
            "nullable": true,
            "readOnly": true,
            "type": "integer"
          },
          "vendor": {
            "nullable": true,
            "readOnly": true,
            "type": "integer"
          }
        },
        "required": [
          "created_at",
          "id",
          "name",
          "sku",
          "updated_at",
          "user",
          "vendor"
        ],
        "type": "object"
      },
