    "components": {
        "schemas": {
            "CardDetails": {
                "type": "object",
                "description": "Serializer for the CardDetails nested object.",
                "properties": {
                    "psa_grade": {
                        "type": "string",
                        "format": "decimal",
                        "pattern": "^-?\\d{0,2}(?:\\.\\d{0,1})?$",
                        "nullable": true,
                        "description": "Numeric grade, e.g. 8.5 or 10.0"
                    },
                    "condition": {
                        "type": "string",
                        "nullable": true,
                        "description": "Human-readable condition (e.g., Mint, Near Mint).",
                        "maxLength": 50
                    },
                    "external_ids": {
                        "type": "string",
                        "nullable": true,
                        "description": "JSON string of external IDs (portable across DBs)"
                    },
                    "last_estimated_at": {
                        "type": "string",
                        "format": "date-time",
                        "nullable": true
                    },
                    "language": {
                        "type": "string",
                        "nullable": true,
                        "maxLength": 50
                    },
                    "release_date": {
                        "type": "string",
                        "format": "date",
                        "nullable": true
                    },
                    "print_run": {
                        "type": "string",
                        "nullable": true,
                        "description": "Optional print run or edition info.",
                        "maxLength": 100
                    },
                    "market_region": {
                        "type": "string",
                        "nullable": true,
                        "maxLength": 100
                    },
                    "notes": {
                        "type": "string",
                        "nullable": true,
                        "description": "Internal notes about the card"
                    }
                }
            },
            "Collectible": {
                "type": "object",
                "description": "Serializer for the Collectible model with nested card details support.",
                "properties": {
                    "id": {
                        "type": "integer",
                        "readOnly": true
                    },
                    "user": {
                        "type": "integer",
                        "readOnly": true,
                        "nullable": true,
                        "description": "The user/vendor who owns this inventory item."
                    },
                    "vendor": {
                        "type": "integer",
                        "readOnly": true,
                        "nullable": true
                    },
                    "name": {
                        "type": "string",
                        "description": "The common name of the collectible item.",
                        "maxLength": 255
                    },
                    "sku": {
                        "type": "string",
                        "description": "Stock Keeping Unit (Unique Identifier).",
                        "maxLength": 50
                    },
                    "description": {
                        "type": "string",
                        "nullable": true
                    },
                    "condition": {
                        "type": "string",
                        "nullable": true,
                        "description": "Human-readable condition (e.g., Mint, Near Mint).",
                        "maxLength": 100
                    },
                    "category": {
                        "type": "string",
                        "nullable": true,
                        "description": "High-level category (e.g., TCG, Video Games, Collectibles).",
                        "maxLength": 100
                    },
                    "image_url": {
                        "type": "string",
                        "format": "uri",
                        "nullable": true,
                        "description": "Public image URL stored in Supabase.",
                        "maxLength": 200
                    },
                    "quantity": {
                        "type": "integer",
                        "maximum": 2147483647,
                        "minimum": -2147483648,
                        "description": "Current number of units in stock."
                    },
                    "intake_price": {
                        "type": "string",
                        "format": "decimal",
                        "pattern": "^-?\\d{0,8}(?:\\.\\d{0,2})?$",
                        "description": "The price paid for the item (cost basis)."
                    },
                    "price": {
                        "type": "string",
                        "format": "decimal",
                        "pattern": "^-?\\d{0,8}(?:\\.\\d{0,2})?$",
                        "description": "The current market value or selling price."
                    },
                    "projected_price": {
                        "type": "string",
                        "format": "decimal",
                        "pattern": "^-?\\d{0,8}(?:\\.\\d{0,2})?$",
                        "description": "The projected price based on market trends."
                    },
                    "card_details": {
                        "$ref": "#/components/schemas/CardDetails"
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "readOnly": true
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time",
                        "readOnly": true
                    }
                },
                "required": [
                    "created_at",
                    "id",
                    "name",
                    "sku",
                    "updated_at",
                    "user",
                    "vendor"
                ]
            },
            "PatchedCollectible": {
                "type": "object",
                "description": "Serializer for the Collectible model with nested card details support.",
                "properties": {
                    "id": {
                        "type": "integer",
                        "readOnly": true
                    },
                    "user": {
                        "type": "integer",
                        "readOnly": true,
                        "nullable": true,
                        "description": "The user/vendor who owns this inventory item."
                    },
                    "vendor": {
                        "type": "integer",
                        "readOnly": true,
                        "nullable": true
                    },
                    "name": {
                        "type": "string",
                        "description": "The common name of the collectible item.",
                        "maxLength": 255
                    },
                    "sku": {
                        "type": "string",
                        "description": "Stock Keeping Unit (Unique Identifier).",
                        "maxLength": 50
                    },
                    "description": {
                        "type": "string",
                        "nullable": true
                    },
                    "condition": {
                        "type": "string",
                        "nullable": true,
                        "description": "Human-readable condition (e.g., Mint, Near Mint).",
                        "maxLength": 100
                    },
                    "category": {
                        "type": "string",
                        "nullable": true,
                        "description": "High-level category (e.g., TCG, Video Games, Collectibles).",
                        "maxLength": 100
                    },
                    "image_url": {
                        "type": "string",
                        "format": "uri",
                        "nullable": true,
                        "description": "Public image URL stored in Supabase.",
                        "maxLength": 200
                    },
                    "quantity": {
                        "type": "integer",
                        "maximum": 2147483647,
                        "minimum": -2147483648,
                        "description": "Current number of units in stock."
                    },
                    "intake_price": {
                        "type": "string",
                        "format": "decimal",
                        "pattern": "^-?\\d{0,8}(?:\\.\\d{0,2})?$",
                        "description": "The price paid for the item (cost basis)."
                    },
                    "price": {
                        "type": "string",
                        "format": "decimal",
                        "pattern": "^-?\\d{0,8}(?:\\.\\d{0,2})?$",
                        "description": "The current market value or selling price."
                    },
                    "projected_price": {
                        "type": "string",
                        "format": "decimal",
                        "pattern": "^-?\\d{0,8}(?:\\.\\d{0,2})?$",
                        "description": "The projected price based on market trends."
                    },
                    "card_details": {
                        "$ref": "#/components/schemas/CardDetails"
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "readOnly": true
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time",
                        "readOnly": true
                    }
                }
            },
            "PatchedVendor": {
                "type": "object",
                "description": "Serializer for vendor CRUD.",
                "properties": {
                    "id": {
                        "type": "integer",
                        "readOnly": true
                    },
                    "name": {
                        "type": "string",
                        "maxLength": 200
                    },
                    "slug": {
                        "type": "string",
                        "readOnly": true,
                        "pattern": "^[-a-zA-Z0-9_]+$"
                    },
                    "description": {
                        "type": "string",
                        "nullable": true
                    },
                    "contact_info": {
                        "type": "string",
                        "nullable": true,
                        "description": "Free-form contact metadata (JSON string ok)."
                    },
                    "is_active": {
                        "type": "boolean"
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "readOnly": true
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time",
                        "readOnly": true
                    }
                }
            },
            "Register": {
                "type": "object",
                "description": "Serializer for user registration.",
                "properties": {
                    "username": {
                        "type": "string",
                        "maxLength": 150
                    },
                    "email": {
                        "type": "string",
                        "format": "email"
                    },
                    "password": {
                        "type": "string",
                        "writeOnly": true,
                        "minLength": 8
                    }
                },
                "required": [
                    "email",
                    "password",
                    "username"
                ]
            },
            "TokenObtainPair": {
                "type": "object",
                "properties": {
                    "username": {
