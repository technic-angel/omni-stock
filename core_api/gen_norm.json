{
  "components": {
    "schemas": {
      "CardDetails": {
        "description": "Serializer for the CardDetails nested object.",
        "properties": {
          "condition": {
            "description": "Human-readable condition (e.g., Mint, Near Mint).",
            "maxLength": 50,
            "nullable": true,
            "type": "string"
          },
          "external_ids": {
            "description": "JSON string of external IDs (portable across DBs)",
            "nullable": true,
            "type": "string"
          },
          "language": {
            "maxLength": 50,
            "nullable": true,
            "type": "string"
          },
          "last_estimated_at": {
            "format": "date-time",
            "nullable": true,
            "type": "string"
          },
          "market_region": {
            "maxLength": 100,
            "nullable": true,
            "type": "string"
          },
          "notes": {
            "description": "Internal notes about the card",
            "nullable": true,
            "type": "string"
          },
          "print_run": {
            "description": "Optional print run or edition info.",
            "maxLength": 100,
            "nullable": true,
            "type": "string"
          },
          "psa_grade": {
            "description": "Numeric grade, e.g. 8.5 or 10.0",
            "format": "decimal",
            "nullable": true,
            "pattern": "^-?\\d{0,2}(?:\\.\\d{0,1})?$",
            "type": "string"
          },
          "release_date": {
            "format": "date",
            "nullable": true,
            "type": "string"
          }
        },
        "type": "object"
      },
      "Collectible": {
        "description": "Serializer for the Collectible model.\n\nSupports writable nested `card_details` payloads. The nested handling is\nintentionally explicit and minimal: create/update will handle a provided\n`card_details` object by creating or updating the related CardDetails row.",
        "properties": {
          "card_details": {
            "$ref": "#/components/schemas/CardDetails"
          },
          "current_price": {
            "description": "The current market value or selling price.",
            "format": "decimal",
            "pattern": "^-?\\d{0,8}(?:\\.\\d{0,2})?$",
            "type": "string"
          },
          "description": {
            "nullable": true,
            "type": "string"
          },
          "id": {
            "readOnly": true,
            "type": "integer"
          },
          "image": {
            "description": "Image of the collectible item.",
            "format": "uri",
            "nullable": true,
            "type": "string"
          },
          "intake_price": {
            "description": "The price paid for the item (cost basis).",
            "format": "decimal",
            "pattern": "^-?\\d{0,8}(?:\\.\\d{0,2})?$",
            "type": "string"
          },
          "last_updated": {
            "format": "date-time",
            "readOnly": true,
            "type": "string"
          },
          "location": {
            "description": "Physical location (e.g., 'Binder 3', 'Shelf A')",
            "maxLength": 100,
            "nullable": true,
            "type": "string"
          },
          "name": {
            "description": "The common name of the collectible item.",
            "maxLength": 255,
            "type": "string"
          },
          "projected_price": {
            "description": "The projected price based on market trends.",
            "format": "decimal",
            "pattern": "^-?\\d{0,8}(?:\\.\\d{0,2})?$",
            "type": "string"
          },
          "quantity": {
            "description": "Current number of units in stock.",
            "maximum": 2147483647,
            "minimum": -2147483648,
            "type": "integer"
          },
          "sku": {
            "description": "Stock Keeping Unit (Unique Identifier).",
            "maxLength": 50,
            "type": "string"
          },
          "user": {
            "description": "The user/vendor who owns this inventory item.",
            "nullable": true,
            "type": "integer"
          },
          "vendor": {
            "nullable": true,
            "type": "integer"
          }
        },
        "required": [
          "id",
          "last_updated",
          "name",
          "sku"
        ],
        "type": "object"
      },
      "PatchedCollectible": {
