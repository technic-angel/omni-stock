          },
          "last_updated": {
            "format": "date-time",
            "readOnly": true,
            "type": "string"
          },
          "location": {
            "description": "Physical location (e.g., 'Binder 3', 'Shelf A')",
            "maxLength": 100,
            "nullable": true,
            "type": "string"
          },
          "name": {
            "description": "The common name of the collectible item.",
            "maxLength": 255,
            "type": "string"
          },
          "projected_price": {
            "description": "The projected price based on market trends.",
            "format": "decimal",
            "pattern": "^-?\\d{0,8}(?:\\.\\d{0,2})?$",
            "type": "string"
